{
  "name": "emotive",
  "version": "1.0.0",
  "description": "Dynamic \"css in js\" without interpolation or language injection",
  "author": "zanomate",
  "license": "ISC",
  "scripts": {
    "clean": "shx rm -rf dist/",
    "gen": "ts-node --project ./src/tsconfig.json -r tsconfig-paths/register src/index.ts",
    "dist": "webpack",
    "build": "npm run clean && npm run gen && npm run dist"
  },
  "devDependencies": {
    "@types/node": "^10.12.0",
    "@types/prettier": "^1.13.2",
    "copy-webpack-plugin": "^4.5.2",
    "css-syntax-parser": "^1.3.1",
    "mdn-data": "^2.0.0",
    "prettier": "^1.14.3",
    "shx": "^0.3.2",
    "ts-loader": "^5.1.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "typescript": "^3.1.3",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "csstype": "^2.5.7"
  }
}
