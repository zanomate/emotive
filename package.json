{
  "name": "emotive",
  "version": "1.0.1",
  "description": "Dynamic \"css in js\" without interpolation or language injection",
  "author": "zanomate",
  "license": "ISC",
  "scripts": {
    "clean": "shx rm -rf dist/*",
    "generate": "ts-node --project ./src/tsconfig.json -r tsconfig-paths/register src/index.ts",
    "rollup": "rollup -c",
    "types": "tsc -d --outDir dist build/emotive.ts",
    "cp": "shx cp build/package.json dist && shx cp README.md dist",
    "build": "npm run clean && npm run generate && npm run types && npm run rollup && npm run cp"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@types/node": "^10.12.0",
    "@types/prettier": "^1.13.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-syntax-parser": "^1.5.1",
    "mdn-data": "^2.0.0",
    "prettier": "^1.14.3",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-babel-minify": "^6.1.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.17.2",
    "shx": "^0.3.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.1.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "typescript": "^3.1.3",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0"
  }
}
